<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <stroke
                android:width="1dp"
                android:color="#05000000" />
            <corners android:radius="@dimen/_20sdp" />
        </shape>
    </item>

    <item>
        <shape>
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <stroke
                android:width="1dp"
                android:color="#10000000" />
            <corners android:radius="@dimen/_20sdp" />
        </shape>
    </item>

    <item>
        <shape>
            <corners android:radius="@dimen/_20sdp" />
            <solid android:color="@color/white" />
            <stroke
                android:width="@dimen/_2sdp"
                android:color="@color/white" />
            <padding
                android:left="@dimen/_2sdp"
                android:right="@dimen/_2sdp"
                android:top="@dimen/_2sdp" />
        </shape>
    </item>

    <item>
        <shape>
            <padding
                android:left="@dimen/_2sdp"
                android:right="@dimen/_2sdp"
                android:top="@dimen/_2sdp" />
            <corners android:radius="@dimen/_20sdp" />
            <solid android:color="@color/white" />
        </shape>
    </item>
</layer-list>